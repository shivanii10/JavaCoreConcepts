package fisglobal;

// process of supressing bas class over derived class when they have same methods.

class Father{  
	String Surname;
	String name;
	Father(){
		this.Surname="kapoor";
		this.name="abhay";
	}
	
	public void calArea() {
		System.out.println("The area is : "+(length+breadth));
	}
}

class Son extends Father{  
	int height;
	Son(){
		height=3;
	}
	
	public void calArea() {
		System.out.println("The area is : "+(length*breadth));
	}
	
	public void calVolume() {
		System.out.println("The volume is : "+(length*breadth*height));
	}
}

public class Overriding {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Father f =new Father();
		f.calArea();
		
		Volume v = new Volume();
		v.calArea();  // is called from Area
		v.calVolume();  // is from its own method called Volume
	}

}